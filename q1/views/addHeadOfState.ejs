<h1>Add Head Of State</h1>

<% if(errors != undefined){ %>
  <ul>
      <% errors.forEach((error) => { %>
          <li>
              <%= error.msg %>
          </li><% }) %>
  </ul>
<% } %>

<form action="/addHeadOfState" method="POST">
  <label>Country Code</label>
  <input type="text" name="_id"><br>
  <label>Head Of State</label>
  <input type="text" name="headOfState"><br>
  <input type="submit" value="Add">
</form>

<a href="./">Home</a>