<h1>Heads Of State</h1>

<a href="/addHeadOfState"><button>Add Head Of State</button></a>

<br><br>

<table style="border: 2px ridge black;">
    <tr>
        <th style="border: 2px ridge black;">Country Code</th>
        <th style="border: 2px ridge black;">Head Of State</th>
    </tr>
    <% headsOfState.forEach((headsOfState) =>
    { %>
    <tr>
        <td style="border: 2px ridge black;"><%= headsOfState._id %></td>
        <td style="border: 2px ridge black;"><%= headsOfState.headOfState %></td>
    </tr>
    <% }) %>
</table>

